.footer{
	margin: 7em 3em 3em;
	border-top: solid 1px @grayBorder;
	padding: 1em;
	.main-partners {
		p {
			text-align: left;
		}
	}
	.organized-by {
		p {
			text-align: center;
			img {
				display: inline-block;
			}
			&.copyright {
				border-top: solid 1px @grayBorder;
			}
		}
	}
	.media-partners {
		p {
			text-align: right;
		}
	}
}